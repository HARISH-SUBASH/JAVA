import java.util.*;
class prog{
    public static void main(String[] args){
        Scanner h=new Scanner(System.in);
        int a=h.nextInt();
        int[] arr=new int[a];
        for(int i=0;i<a;i++){
            arr[i]=h.nextInt();
        }
        System.out.println("ArrayList: " +Arrays.toString(arr));
        System.out.print("First : " + arr[0] +",");
        System.out.print(" Last : "+arr[a-1]);
    }
}
